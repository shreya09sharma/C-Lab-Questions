#include <stdio.h>

int main() {
    int n;
printf("enter the elements of an array\n");
scanf("%d", &n);
int arr[n];
int array[n];
for(int i=0;i<n;i++)
{
    printf("elements %d",i+1);
    scanf("%d",&arr[i]);
    
}
for(int i=0;i<n;i++)
{
    array[i]==arr[i];
}
printf("\n the original array is\n");
for(int i=0;i<n;i++)
{
    printf("%d", &arr[i]);
}
printf("\n the copied element is\n");
for(int i=0;i<n;i++)
{
    printf("%d", array[i]);
}

    return 0;
}
