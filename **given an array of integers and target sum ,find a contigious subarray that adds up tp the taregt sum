#include <stdio.h>

int main() {
    int n;
    printf("Enter the number of elements in the array: ");
    scanf("%d", &n);

    int arr[n];
    for (int i = 0; i < n; i++) 
    {
        printf("Element %d: ", i + 1);
        scanf("%d", &arr[i]);
    }

    printf("\nThe elements of the array are:\n");
    for (int i = 0; i < n; i++) 
    {
        printf("%d ", arr[i]);
    }
    printf("\n");

    int sum;
    printf("\nEnter target sum: ");
    scanf("%d", &sum);

    int f = 0;

    for (int s = 0; s < n; s++)
    {
        int ssum = 0;
        for (int l = s; l < n; l++) 
        {
            ssum += arr[l];

            if (ssum == sum) 
            {
                
                for (int k = s; k <= l; k++) 
                {
                    printf("%d ", arr[k]);
                }
                printf("\n");
                f= 1;
                break;
            }
        }
        if (f==1) break;
    }

    if (f==0)
        printf("\nNo subarray found with sum %d\n", sum);

    return 0;
}
